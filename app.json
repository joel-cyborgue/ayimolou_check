{
  "expo": {
    "name": "Ayimolou",
    "slug": "Ayimolou",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/round_icon_sh.png",
    "scheme": "Ayimolou",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ayimolou_1.png",
        "backgroundColor": "#FFE223"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.joeldate39.Ayimolou"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ayimolou_1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ayimolou_1.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "yellow"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": ""
      }
    }
  }
}
